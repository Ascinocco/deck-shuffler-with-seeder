{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "importHelpers": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "paths": {
      "tslib": ["./node_modules/tslib/tslib.d.ts"]
    },
    "noImplicitAny": true
  },
  "lib": ["es2015"]
}
